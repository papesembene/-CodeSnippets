{
    "name": "mr-sem-s/mini-framework-php",
    "description": "A simple PHP framework for building web applications.",
    "type": "project",
    "keywords": [
        "php",
        "framework",
        "web",
        "application"
    ],
    "autoload": {
        "psr-4": {
           
            "App\\Core\\": "App/core",
            "App\\Controllers\\": "src/controllers",
            "App\\Entities\\": "src/entities",
            "App\\Repositories\\": "src/repositories",
            "App\\Services\\": "src/services",
            "App\\Core\\Abstract\\": "App/core/abstract",
            "App\\Core\\Middlewares\\": "App/core/middlewares",
            "App\\Core\\Interfaces\\": "App/core/Interface",
            "App\\Migration\\": "database/migrations",
            "App\\Seeders\\": "database/seeders"


        }
    },
    "authors": [
        {
            "name": "pape sembene",
            "email": "sembenpape4@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/yaml": "^5.0",
        "vlucas/phpdotenv": "^5.6",
        "mrsems/router-php": "^1.0",
        "mrsems/paginator-php": "^1.0"


        
       
    },
    "minimum-stability": "stable",
    "scripts": {
        "serve": "php -S localhost:8000 -t public",
        "database:migrate": "@php bin/migrate",
        "database:seed": "@php bin/seed"
    }
}
